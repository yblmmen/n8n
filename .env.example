# The top level domain to serve from

DOMAIN_NAME=innerinfo.cloud

# 서브도메인입니다
# The subdomain to serve from
SUBDOMAIN=n8n

# NEWYORK이 Default입니다. 원하는 도시로 변경하세요
# Optional timezone to set which gets used by Cron. 
GENERIC_TIMEZONE=Asia/Seoul

# 인증받을 이메일 주소를 입력하세요 
# The email address to use for the TLS/SSL certificate creation. New York is the default value if not set
SSL_EMAIL=yblmmen@gmail.com

# PostgreSQL 설정
# set PostgreSQL (change your own key)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=berea6922
POSTGRES_DB=n8n

# N8N 설정 
# set N8N key
N8N_ENCRYPTION_KEY=n8n-encryption-key
N8N_USER_MANAGEMENT_JWT_SECRET=n8n-jwt-secret

